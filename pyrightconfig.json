{
  "include": ["src", "tests", "example"],
  "exclude": ["build", "dist", ".venv"],
  "executionEnvironments": [
    {
      "root": "./src",
      "extraPaths": ["./src"]
    }
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": true,
  "reportOptionalMemberAccess": true,
  "typeCheckingMode": "strict",
  "venvPath": ".",
  "venv": ".venv"
}
